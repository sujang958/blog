---
import Layout from "@layouts/Layout.astro"
import { getCollection } from "astro:content"

const posts = await getCollection("hacking")
---

<Layout title="Write-Ups by Sujang" class="font-intel-mono">
  <p class="text-4xl">Posts about Hacking</p>
  <div class="py-3"></div>
  {posts.map((post) => <div>{post.data.title}</div>)}
</Layout>
